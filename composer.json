{
    "name": "niclashedam/eloquent-round",
    "description": "Automatic rounding of decimals/floats/doubles in Laravel Eloquent",
    "type": "library",
    "require": {
        "php": ">=7.2",
        "illuminate/database": "^5|^6"
    },
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Niclas Hedam",
            "email": "niclas@frax.dk"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "*",
        "php-coveralls/php-coveralls": "*"
    },
    "scripts": {
        "test": "./vendor/phpunit/phpunit/phpunit ./tests"
    },
    "autoload":{
        "psr-4":{
            "NiclasHedam\\EloquentRound\\" : "src/NiclasHedam/EloquentRound/"
        }
    }
